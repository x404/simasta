<!-- =modal callback -->
<div class="md-modal md-effect-9" id="callback" tabindex="-1" role="dialog" aria-hidden="true">
	<div class="md-content modal-dialog" role="document">
		<div class="container">
			<div class="modal__body">
				<form action="#" method="post" class="form-policy" id="callback-form">
					<fieldset>
						<label for="cbname" class="visually-hidden">как вас зовут</label>
						<input type="text" id="cbname" name="name" class="form-control required" required="required" value="" placeholder="как вас зовут" tabindex="0" autofocus />

						<label for="cbtel" class="visually-hidden">телефон</label>
						<input type="tel" id="cbtel" name="tel" class="form-control tel required" required="required" value="" placeholder="телефон" tabindex="1" />
						<input type="hidden" name="workemail" value="">

						<div class="policy">
							<input type="checkbox" id="cbcheckbox" value="" name="checkbox" checked="checked" class="user-access">
							<label for="cbcheckbox">Я Согласен на обработку персональных данных и принимаю условия <a href="#.pdf" title="" target="_blank">Пользовательского соглашения</a></label>
						</div>
						<button type="submit" class="submit btn" tabindex="2"><span>перезвонить мне</span></button>
					</fieldset>
				</form>
			</div>
			<button type="button" data-dismiss="modal" class="md-close close" aria-label="Закрыть"></button>
		</div>
	</div>
</div>
<!-- =/modal callback -->

